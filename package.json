{
  "name": "diamond-sweeper",
  "version": "1.0.0",
  "description": "quintype-frontend-coding-challenge",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirajmchauhan/diamond-sweeper.git"
  },
  "keywords": [
    "frontend",
    "js",
    "coding"
  ],
  "author": "Niraj Chauhan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nirajmchauhan/diamond-sweeper/issues"
  },
  "homepage": "https://github.com/nirajmchauhan/diamond-sweeper#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@testing-library/dom": "^6.10.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "jest-transform-stub": "^2.0.0",
    "node-sass": "^4.13.0",
    "prettier": "1.19.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "common"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
